ifndef _MK_CLEAN

_MK_CLEAN = 1

# ---------------------------------------------------------------------------- #

.PHONY: clean

clean: FORCE
	-$(RM) $(BIN_TARGETS) $(LIB_TARGETS) $(TEST_TARGETS) $(DEPEND_TARGETS)
	-$(RM) $(ALL_OBJS)
	-$(RM) $(BUILT_SRCS)
	-$(RM) -r $(PREFIX)
	-$(RM) -r $(TEST_MANIFEST_LOCKS)
	-$(RM) result *.db gmon.out *.log *.gcda *.gcno coverage.info


# ---------------------------------------------------------------------------- #

endif  # ifndef _MK_CLEAN


